#include<stdio.h>
void main(){
    int i;
    for(i=0; i<=20; i++){
         if(i%2==0){
             int soma= i+i;
             
          
        }
    }
     printf("A soma dos numeros pares entre 0 e 20 é: %d \n", soma);
    
}


