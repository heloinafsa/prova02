#include<stdio.h>
void main(){
    int valor, centena, dezena, unidade;
    printf("informe o valor da centena do seu saque :\n");
    scanf("%d", &centena);
    printf("informe o valor da dezena do seu saque:\n");
    scanf("%d", &dezena);
    printf("informe o valor da unidade inteira do seu saque:\n");
    scanf("%d", &unidade);
    
    if(centena%100==0){
        printf("a quantidade minima de notas é:%d\n", centena/100);
    }
    if(dezena%50==0){
        printf("a quantidade minima de notas é:%d\n ", dezena/50);
    }   
    if(dezena%20==0){
        printf("a quantidade minima de notas é:%d\n", dezena/20);
    }
    if(dezena%10==0){
       printf("a quantidade minima de notas é:%d\n", dezena/10);
    }
    if(unidade%5==0){
        printf("a quantidade minima de notas é:%d\n", unidade/5);
    }
    if(unidade%2==0){
        printf("a quantidade minina de notas é:%d\n", unidade/2);
    }
    valor=unidade+centena+dezena;
    printf("quantidade de cedulas:%d ",  valor=unidade+centena+dezena);
        
    
}
